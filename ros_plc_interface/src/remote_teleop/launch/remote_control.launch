<?xml version="1.0" ?>
<launch>

  <param name = "radius" type = "double" value = "0.075" />
  <param name = "wheel_dist" type = "double" value = "0.435" />
  <param name = "odom_pub_rate" type = "double" value = "10" />
  <param name = "plc_ip_addr" type = "string" value = "192.168.0.123" />
  <param name = "motor1_addr" type = "string" value = "MD6.0" />
  <param name = "motor2_addr" type = "string" value = "MD10" />
  <param name = "encoder1_addr" type = "string" value = "MD14" />
  <param name = "encoder2_addr" type = "string" value = "MD18" />

  <!-- run teleop_plc -->  
  <node pkg="remote_teleop" type="teleop_plc.py" name="teleop_plc" output="screen"/>

  <!-- run teleop_twist_keyboard -->
  <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop_twist_keyboard" output="screen"/>

</launch>